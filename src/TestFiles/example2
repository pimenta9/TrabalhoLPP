class Pessoa
    vars id, idade

    method retorna(x)
    begin
        return x
    end-method
end-class

class Animal
    vars qtdDedos, idade
end-class

main()
vars numUm, numDois, numTres, p, a
begin
    numUm = 40
    numDois = 20

    if numUm lt numDois then
        numTres = numUm + numDois
    else
        numTres = numUm - numDois
    end-if

    p = new Pessoa
    a = p.retorna(numTres)

end