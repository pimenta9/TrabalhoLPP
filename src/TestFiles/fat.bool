class Fat
method fat(x)
vars b, a, y, z, w, c
begin
    b = 0

    c = new Fat

    if x eq b then
        b = 1
        return c
    end-if

    a = 1
    w = x - a
    z = c.fat(w)
    y = x * z

    return y

end-method
end-class

main()
vars x, y, z
begin
    x = new Fat
    y = 5
    z = x.fat(y)
    io.print(z)
end